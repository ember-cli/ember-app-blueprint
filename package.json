{
  "name": "@ember/app-blueprint",
  "version": "6.10.0-alpha.9",
  "description": "Blueprint for next generation of Ember apps",
  "keywords": [
    "ember-blueprint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ember-cli/ember-app-blueprint.git"
  },
  "license": "MIT",
  "author": "",
  "main": "index.js",
  "scripts": {
    "lint": "concurrently 'pnpm:lint:*(!fix)'",
    "lint:fix": "pnpm lint:eslint && pnpm format",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "format": "prettier . --write",
    "new:app": "pnpm dlx ember-cli@latest new my-app --blueprint .",
    "update:all": "pnpm dlx update-blueprint-deps --filter . ./files/package.json",
    "update:alpha": "pnpm dlx update-blueprint-deps --ember-source alpha --ember-cli alpha --ember-data alpha ./files/package.json",
    "update:beta": "pnpm dlx update-blueprint-deps --ember-source beta --ember-cli beta --ember-data beta ./files/package.json",
    "test": "vitest"
  },
  "release-plan": {
    "semverIncrementAs": {
      "minor": "prerelease",
      "patch": "prerelease"
    },
    "semverIncrementTag": "alpha",
    "publishTag": "alpha"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "ejs": "^3.1.10",
    "ember-cli-string-utils": "^1.1.0",
    "lodash": "^4.17.21",
    "sort-package-json": "^3.5.1",
    "walk-sync": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "concurrently": "^9.1.2",
    "ember-cli": "^6.9.1",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "execa": "^9.1.0",
    "fixturify": "^3.0.0",
    "globals": "^15.3.0",
    "jsonc-parser": "^3.3.1",
    "prettier": "^3.2.5",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "release-plan": "^0.17.2",
    "strip-ansi": "^7.1.0",
    "tmp-promise": "^3.0.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.20.0"
}
